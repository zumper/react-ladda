{
  "name": "@zumper/react-ladda",
  "version": "7.1.2",
  "description": "React wrapper for Ladda buttons",
  "keywords": [
    "react",
    "ladda",
    "button"
  ],
  "license": "MIT",
  "author": "Jason Sommer",
  "homepage": "https://github.com/zumper/react-ladda",
  "repository": "github:zumper/react-ladda",
  "bugs": "https://github.com/zumper/react-ladda/issues",
  "main": "lib/index.js",
  "unpkg": "dist/zumper-react-ladda.js",
  "module": "es/index.js",
  "files": [
    "dist",
    "lib",
    "src",
    "es"
  ],
  "scripts": {
    "copy:ladda": "node ./scripts/copy-ladda.js",
    "build:commonjs": "cross-env NODE_ENV=production MODULES_ENV=commonjs babel src --out-dir lib",
    "build:es": "cross-env NODE_ENV=production MODULES_ENV=esmodules babel src --out-dir es",
    "build:umd": "cross-env NODE_ENV=production rollup -c",
    "build": "yarn copy:ladda && yarn build:commonjs && yarn build:es && yarn build:umd",
    "clean": "rimraf lib dist es coverage",
    "format": "prettier --write \"{example,src,test}/**/*.{js,jsx,ts,tsx}\" .babelrc.js rollup.config.js .eslintrc.json README.md \"docs/**/*.md\" && yarn lint --fix || true",
    "lint": "eslint src test",
    "prepare": "yarn clean && yarn build",
    "pretest": "yarn lint",
    "preversion": "yarn format",
    "postversion": "git push && git push --tags",
    "test": "yarn copy:ladda && jest",
    "coverage": "jest --coverage"
  },
  "peerDependencies": {
    "prop-types": "^15.0.0",
    "react": "^16.4.0"
  },
  "dependencies": {
    "@babel/runtime": "^7.3.0",
    "ladda": "^2.0.1"
  },
  "devDependencies": {
    "@babel/cli": "^7.2.3",
    "@babel/core": "^7.2.2",
    "@zumper/babel-preset-react-app": "7.0.1-zumper.2",
    "babel-core": "^7.0.0-bridge.0",
    "babel-eslint": "^10.0.1",
    "babel-jest": "^23.6.0",
    "cross-env": "^5.2.0",
    "enzyme": "^3.8.0",
    "enzyme-adapter-react-16": "^1.9.1",
    "eslint": "^5.13.0",
    "eslint-config-prettier": "^4.0.0",
    "eslint-config-react-app": "^3.0.7",
    "eslint-config-standard": "^12.0.0",
    "eslint-config-standard-react": "^7.0.2",
    "eslint-plugin-flowtype": "^2.50.3",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-jest": "^22.2.2",
    "eslint-plugin-jsx-a11y": "^6.1.2",
    "eslint-plugin-node": "^8.0.1",
    "eslint-plugin-prettier": "^3.0.1",
    "eslint-plugin-promise": "^4.0.1",
    "eslint-plugin-react": "^7.12.4",
    "eslint-plugin-standard": "^4.0.0",
    "fs-extra": "^7.0.1",
    "jest": "^23.6.0",
    "jest-dom": "^3.0.0",
    "jest-environment-enzyme": "^7.0.1",
    "jest-environment-jsdom": "^23.4.0",
    "jest-environment-jsdom-global": "^1.1.0",
    "jest-enzyme": "^7.0.1",
    "jest-pnp-resolver": "^1.0.2",
    "jest-resolve": "^23.6.0",
    "jsdom": "^13.1.0",
    "prettier": "^1.16.4",
    "prop-types": "^15.6.2",
    "react": "^16.8.1",
    "react-app-polyfill": "^0.2.1",
    "react-dom": "^16.8.1",
    "react-test-renderer": "^16.8.1",
    "replace-in-file": "^3.4.3",
    "rimraf": "^2.6.3",
    "rollup": "^1.1.2",
    "rollup-plugin-babel": "^4.3.2",
    "rollup-plugin-commonjs": "^9.2.0",
    "rollup-plugin-node-resolve": "^4.0.0",
    "rollup-plugin-replace": "^2.1.0",
    "rollup-plugin-terser": "^4.0.4"
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{js,jsx,ts,tsx}",
      "!src/**/*.d.ts"
    ],
    "coverageDirectory": "<rootDir>/coverage/",
    "coveragePathIgnorePatterns": [
      "<rootDir>/dist",
      "<rootDir>/es",
      "<rootDir>/lib",
      "<rootDir>/node_modules"
    ],
    "resolver": "jest-pnp-resolver",
    "setupFiles": [
      "react-app-polyfill/jsdom"
    ],
    "testMatch": [
      "<rootDir>/test/**/*.spec.js"
    ],
    "testPathIgnorePatterns": [
      "<rootDir>/dist",
      "<rootDir>/es",
      "<rootDir>/lib",
      "<rootDir>/node_modules"
    ],
    "setupTestFrameworkScriptFile": "jest-enzyme",
    "testEnvironment": "enzyme",
    "testEnvironmentOptions": {
      "enzymeAdapter": "react16"
    },
    "testURL": "https://localhost"
  }
}
