{
  "name": "@zumper/react-ladda",
  "version": "7.2.1",
  "description": "React wrapper for Ladda buttons",
  "keywords": [
    "react",
    "ladda",
    "button"
  ],
  "license": "MIT",
  "author": "Jason Sommer",
  "homepage": "https://github.com/zumper/react-ladda",
  "repository": "github:zumper/react-ladda",
  "bugs": "https://github.com/zumper/react-ladda/issues",
  "main": "lib/index.js",
  "unpkg": "dist/zumper-react-ladda.js",
  "module": "es/index.js",
  "files": [
    "dist",
    "lib",
    "src",
    "es"
  ],
  "scripts": {
    "copy:ladda": "node ./scripts/copy-ladda.js",
    "build:commonjs": "cross-env MODULES_ENV=commonjs babel src --out-dir lib",
    "build:es": "cross-env MODULES_ENV=esmodules babel src --out-dir es",
    "build:umd": "rollup -c",
    "build": "yarn copy:ladda && yarn build:commonjs && yarn build:es && yarn build:umd",
    "clean": "rimraf lib dist es coverage",
    "format": "prettier --write \"{example,src,test}/**/*.{js,jsx,ts,tsx}\" .babelrc.js rollup.config.js .eslintrc.json README.md \"docs/**/*.md\" && yarn lint --fix || true",
    "lint": "eslint src test",
    "dedupe": "npx yarn-deduplicate yarn.lock && yarn install",
    "prepare": "yarn clean && yarn build",
    "pretest": "yarn lint",
    "preversion": "yarn format",
    "postversion": "npm publish --access public",
    "postpublish": "git push origin --all && git push origin --tags",
    "test": "yarn copy:ladda && jest",
    "coverage": "jest --coverage"
  },
  "peerDependencies": {
    "prop-types": "^15.0.0",
    "react": "^16.4.0"
  },
  "dependencies": {
    "@babel/runtime": "^7.6.3",
    "ladda": "^2.0.1"
  },
  "devDependencies": {
    "@babel/cli": "^7.6.4",
    "@babel/core": "^7.6.4",
    "@zumper/babel-preset-react-app": "9.0.2-zumper.19",
    "babel-eslint": "^10.0.3",
    "babel-jest": "^24.9.0",
    "cross-env": "^6.0.3",
    "enzyme": "^3.10.0",
    "enzyme-adapter-react-16": "^1.15.1",
    "eslint": "^6.6.0",
    "eslint-config-prettier": "^6.5.0",
    "eslint-config-react-app": "^5.0.2",
    "eslint-config-standard": "^14.1.0",
    "eslint-config-standard-react": "^9.2.0",
    "eslint-plugin-flowtype": "3.13.0",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-jest": "^23.0.2",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-node": "^10.0.0",
    "eslint-plugin-prettier": "^3.1.1",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-react": "^7.16.0",
    "eslint-plugin-react-hooks": "^2.2.0",
    "eslint-plugin-standard": "^4.0.1",
    "fs-extra": "^8.1.0",
    "jest": "^24.9.0",
    "jest-dom": "^4.0.0",
    "jest-environment-enzyme": "^7.1.2",
    "jest-environment-jsdom": "^24.9.0",
    "jest-environment-jsdom-global": "^1.2.0",
    "jest-enzyme": "^7.1.2",
    "jest-pnp-resolver": "^1.2.1",
    "jest-resolve": "^24.9.0",
    "jsdom": "^15.2.0",
    "lodash": "^4.17.15",
    "prettier": "^1.18.2",
    "prop-types": "^15.7.2",
    "react": "^16.11.0",
    "react-app-polyfill": "^1.0.4",
    "react-dom": "^16.11.0",
    "react-test-renderer": "^16.11.0",
    "replace-in-file": "^4.2.0",
    "rimraf": "^3.0.0",
    "rollup": "^1.26.0",
    "rollup-plugin-babel": "^4.3.3",
    "rollup-plugin-commonjs": "^10.1.0",
    "rollup-plugin-node-resolve": "^5.2.0",
    "rollup-plugin-replace": "^2.2.0",
    "rollup-plugin-terser": "^5.1.2"
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{js,jsx,ts,tsx}",
      "!src/**/*.d.ts"
    ],
    "coverageDirectory": "<rootDir>/coverage/",
    "coveragePathIgnorePatterns": [
      "<rootDir>/dist",
      "<rootDir>/es",
      "<rootDir>/lib",
      "<rootDir>/node_modules"
    ],
    "resolver": "jest-pnp-resolver",
    "setupFiles": [
      "react-app-polyfill/jsdom"
    ],
    "testMatch": [
      "<rootDir>/test/**/*.spec.js"
    ],
    "testPathIgnorePatterns": [
      "<rootDir>/dist",
      "<rootDir>/es",
      "<rootDir>/lib",
      "<rootDir>/node_modules"
    ],
    "setupFilesAfterEnv": [
      "jest-enzyme"
    ],
    "testEnvironment": "enzyme",
    "testEnvironmentOptions": {
      "enzymeAdapter": "react16"
    },
    "testURL": "https://localhost"
  }
}
